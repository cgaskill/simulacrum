{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:8080"
    }
  },
  "homepage": "/admin",
  "scripts": {
    "start": "NODE_PATH=./src env-cmd ./config/.env.development react-scripts start",
    "build": "NODE_PATH=./src env-cmd ./config/.env.production react-scripts build",
    "build-dev": "NODE_PATH=./src env-cmd ./config/.env.development react-scripts build",
    "test": "NODE_PATH=./src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .js,.jsx --ignore-pattern node_modules/"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.2",
    "env-cmd": "^8.0.2",
    "ra-data-json-server": "^2.0.3",
    "ra-data-simple-rest": "^2.0.3",
    "react": "^16.4.1",
    "react-admin": "^2.3.0",
    "react-dom": "^16.4.1",
    "react-google-login": "^3.2.1",
    "react-scripts": "^1.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app",
      "google"
    ],
    "rules": {
      "require-jsdoc": "off",
      "max-len": [
        1,
        160,
        4
      ],
      "babel/new-cap": 1,
      "babel/no-invalid-this": 1,
      "babel/object-curly-spacing": 1,
      "babel/quotes": [
        "warn",
        "single"
      ],
      "babel/semi": 1,
      "babel/no-unused-expressions": 1,
      "no-invalid-this": 0,
      "quotes": 0,
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "@material-ui/core/es/*"
          ]
        }
      ]
    },
    "plugins": [
      "babel",
      "react"
    ]
  }
}
